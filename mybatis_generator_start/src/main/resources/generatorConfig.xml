<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<classPathEntry location="lib/mysql-connector-java-5.1.44.jar" />
	<context id="DB2Tables" targetRuntime="MyBatis3">
	
	<plugin type="com.sky.mybatis.generator.PaginationPlugin"></plugin>
	
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="addRemarkComments" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://120.27.17.206:3306/bigdata1"
			userId="swd" password="swd12345">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.zhenxue.bigdata.userserivce.entity"
			targetProject="./src">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />

		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.zhenxue.bigdata.userserivce.mapper.xml"
			targetProject="./src">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.zhenxue.bigdata.userserivce.mapper" targetProject="./src">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table schema="bigdata1" tableName="edu_user"/>



	</context>
</generatorConfiguration>